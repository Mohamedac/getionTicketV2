@model IEnumerable<gestionticket_v2.Models.Ticket>

<h2>Technician Tickets</h2>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Description</th>
            <th>Status</th>
            <th>Date Created</th>
            <th>Date Modified</th>
            <!-- Add more columns as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var ticket in Model)
        {
            <tr>
                <td>@ticket.Id</td>
                <td>@ticket.Titre</td>
                <td>@ticket.Description</td>
                <td>@ticket.Statut</td>
                <td>@ticket.DateCreation</td>
                <td>@ticket.DateModification</td>
                <!-- Add more fields as needed -->
            </tr>
        }
    </tbody>
</table>
